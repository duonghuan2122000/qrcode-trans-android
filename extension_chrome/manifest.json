{
    "name": "Test QR Extension",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Extension for testing QR",
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon.png",
            "38": "images/icon.png"
        },
        "default_title": "Test QR Extension",
        "default_popup": "popup.html"
    },
    "permissions": [
        "clipboardWrite",
        "storage"
    ],
    "options_page": "popup.html"
}